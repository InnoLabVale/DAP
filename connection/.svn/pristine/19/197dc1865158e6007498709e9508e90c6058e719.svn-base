package com.innovery.mpm.connection.implementations.util;

import com.innovery.mpm.connection.interfaces.util.ErrorMessagesInterface;


public class ErrorMessages implements ErrorMessagesInterface {

	
	//Non loggato nella sessione corrente, da Action specifica
	private final String _NOT_LOGGED_WARN = "Not logged warn";
	//E' stato impossibile effettuare il login, da ActionReconnection
	private final String _NOT_LOGGED_ERROR = "Not logged error";
	
	//Non connesso a MPM nella sessione corrente, da Action specifica
	private final String _MPM_CONNECTION_WARN = "MPM connection warn";
	//E' stato impossibile effettuare la connessione con MPM, da ActionReconnection
	private final String _MPM_CONNECTION_ERROR = "MPM connection error";
	//Tutti i parametri sono null della session. Ajax reindirizza alla pagina iniziale.
	private final String _MPM_ERROR = "MPM error";

	//E' stato effettuato nuovamente il login, da ActionReconnection
	private final String _RELOGGED = "Relogged";
	
	private final String _USER_NOT_FOUND = "User not found";

	public String get_NOT_LOGGED_WARN() {
		return _NOT_LOGGED_WARN;
	}

	public String get_NOT_LOGGED_ERROR() {
		return _NOT_LOGGED_ERROR;
	}

	public String get_MPM_CONNECTION_WARN() {
		return _MPM_CONNECTION_WARN;
	}

	public String get_MPM_CONNECTION_ERROR() {
		return _MPM_CONNECTION_ERROR;
	}

	public String get_MPM_ERROR() {
		return _MPM_ERROR;
	}

	public String get_RELOGGED() {
		return _RELOGGED;
	}

	public String get_USER_NOT_FOUND() {
		return _USER_NOT_FOUND;
	}
}
